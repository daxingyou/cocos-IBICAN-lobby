{
    // 版本
    "version": "0.0.0",
    // 项目/游戏ID
    "itemId": "should be specified",
    // 游戏支持的朝向:"landscape"(横), "portrait"(竖),如果为""或未规定此字段，则表示支持所有
    // "orientation": "",
    // 热更地址生成规则
    "remoteRule": [
        "remoteUrl",
        "itemId",
        "/"
    ],
    // 打包元数据文件生成地址
    "metaDest": "assets/resources",
    // 是否开启debug
    "debug": false,
    // 是否生成sourceMaps
    "sourceMaps": false,
    // apiLevel
    "apiLevel": "android-24",
    "appABIs": [
        "armeabi-v7a",
        "arm64-v8a"
    ],
    "useDebugKeystore": false,
    "keystoreAlias": "",
    "keystoreAliasPassword": "",
    "keystorePassword": "",
    "keystorePath": ""
}